{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "apps/*": ["./src/apps/*"],
      "components/*": ["./src/components/*"],
      "const/*": ["./src/const/*"],
      "types/*": ["./src/types/*"],
      "utils/*": ["./src/utils/*"],
      "BW_api/*": ["./src/apps/balanceWheel/api/*"],
      "BW_components/*": ["./src/apps/balanceWheel/components/*"],
      "BW_const/*": ["./src/apps/balanceWheel/const/*"],
      "BW_models/*": ["./src/apps/balanceWheel/models/*"],
      "BW_types/*": ["./src/apps/balanceWheel/types/*"],
      "BW_utils/*": ["./src/apps/balanceWheel/utils/*"],
      "MT_subpages/*": ["./src/apps/medicationTaking/subpages/*"],
      "MT_components/*": ["./src/apps/medicationTaking/components/*"],
      "MT_types/*": ["./src/apps/medicationTaking/types/*"],
      "MT_api/*": ["./src/apps/medicationTaking/api/*"],
      "MT_models/*": ["./src/apps/medicationTaking/models/*"],
      "MT_const/*": ["./src/apps/medicationTaking/const/*"],
      "MT_utils/*": ["./src/apps/medicationTaking/utils/*"],
      "CR_subpages/*": ["./src/apps/calendar/subpages/*"],
      "CR_components/*": ["./src/apps/calendar/components/*"],
      "CR_types/*": ["./src/apps/calendar/types/*"],
      "CR_api/*": ["./src/apps/calendar/api/*"],
      "CR_models/*": ["./src/apps/calendar/models/*"],
      "CR_const/*": ["./src/apps/calendar/const/*"],
      "CR_utils/*": ["./src/apps/calendar/utils/*"],
      "VW_subpages/*": ["./src/apps/virtualWardrobe/subpages/*"],
      "VW_components/*": ["./src/apps/virtualWardrobe/components/*"],
      "VW_types/*": ["./src/apps/virtualWardrobe/types/*"],
      "VW_api/*": ["./src/apps/virtualWardrobe/api/*"],
      "VW_models/*": ["./src/apps/virtualWardrobe/models/*"],
      "VW_const/*": ["./src/apps/virtualWardrobe/const/*"],
      "VW_utils/*": ["./src/apps/virtualWardrobe/utils/*"],
      "CL_subpages/*": ["./src/apps/checkLists/subpages/*"],
      "CL_components/*": ["./src/apps/checkLists/components/*"],
      "CL_types/*": ["./src/apps/checkLists/types/*"],
      "CL_api/*": ["./src/apps/checkLists/api/*"],
      "CL_models/*": ["./src/apps/checkLists/models/*"],
      "CL_const/*": ["./src/apps/checkLists/const/*"],
      "CL_utils/*": ["./src/apps/checkLists/utils/*"]
    }
  },
  "include": ["src"]
}
