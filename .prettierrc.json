{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all",
  "importOrder": [
    "^react.*(?<!(css))$",
    "^(?!(apps)|(components)|(const)|(types)|(utils)|(BW_)|(CL_)|(CR_)|(MT_)|(VW_)|(\\.)).*(?<!(css))$",
    "^(?=(apps)|(components)|(const)|(types)|(utils)).*$",
    "^BW_.*$",
    "^CR_.*$",
    "^CL_.*$",
    "^MT_.*$",
    "^VW_.*$",
    "^.*(?<!css)$",
    "^.*(?<!s)css$",
    "^.*scss$",
    ".*"
  ],
  "importOrderSeparation": true
}
