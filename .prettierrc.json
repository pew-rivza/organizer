{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all",
  "importOrder": [
    "^react.*(?<!(css))$",
    "^(?!(apps)|(components)|(const)|(types)|(utils)|(BW_)|(MT_)|(CR_)|(\\.)).*(?<!(css))$",
    "^(?=(apps)|(components)|(const)|(types)|(utils)).*$",
    "^BW_.*$",
    "^MT_.*$",
    "^CR_.*$",
    "^.*(?<!css)$",
    "^.*(?<!s)css$",
    "^.*scss$",
    ".*"
  ],
  "importOrderSeparation": true
}
