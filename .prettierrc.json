{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all",
  "importOrder": [
    "^((utils)|(components)|(types)|(apps)|(const))/(.*)$",
    "^BW_(.*)/(.*)$",
    "^MT_(.*)/(.*)$",
    "^CR_(.*)/(.*)$",
    "^[./](.*)[^(scss)]$",
    "^[./](.*)[(scss)]$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false
}
